{
  "name": "shiny-succotash",
  "version": "0.0.1",
  "description": "React, TypeScript, GraphQL boilerplate",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/index.js",
    "watch": "tsc -w",
    "dev": "yarn build && concurrently --kill-others \"yarn watch\" \"nodemon dist/index.js\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IvaTsu/shiny-succotash.git"
  },
  "keywords": [
    "React, TypeScript, GraphQL, Yarn"
  ],
  "author": "Ivan Tsud",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IvaTsu/shiny-succotash/issues"
  },
  "homepage": "https://github.com/IvaTsu/shiny-succotash#readme",
  "devDependencies": {
    "@types/node": "^14.10.0",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@mikro-orm/cli": "^4.0.1",
    "@mikro-orm/core": "^4.0.1",
    "@mikro-orm/migrations": "^4.0.1",
    "@mikro-orm/postgresql": "^4.0.1",
    "dotenv": "^8.2.0",
    "pg": "^8.3.3"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
